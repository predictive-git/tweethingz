#!/bin/bash

DIR="$(dirname "$0")"
. "${DIR}/config"

export VM_INSTANCE_ID=$(gcloud compute instances describe ${APP_NAME} \
	--zone ${VM_ZONE} --format="value(id)")

gcloud logging read "resource.type=gce_instance AND \
	resource.labels.instance_id=${VM_INSTANCE_ID} " \
	--order="asc"



